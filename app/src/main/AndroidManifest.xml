<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kosaa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ServiceActivities.chatListWorkerLogin"></activity>
        <activity android:name=".ServiceActivities.chatList" />
        <activity android:name=".ServiceActivities.serviceProvideInfoActivity" />
        <activity android:name=".ServiceActivities.chatActivity" />
        <activity android:name=".RegistrationActivities.worker_signupActivity" />
        <activity android:name=".ServiceActivities.serviceProviderListActivity" />
        <activity android:name=".ServiceActivities.HomeActivity" />
        <activity android:name=".RegistrationActivities.LoginActivity" />
        <activity android:name=".RegistrationActivities.SignupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>